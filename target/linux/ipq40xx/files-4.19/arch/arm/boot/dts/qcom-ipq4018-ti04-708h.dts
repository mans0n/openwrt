#include "qcom-ipq4019.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/soc/qcom,tcsr.h>

/ {
	model = "T&Ie TI04-708H";
	compatible = "tnie,ti04-708h";

	memory {
		device_type = "memory";
		reg = <0x80000000 0x10000000>;
	};

	aliases {
		led-boot = &led_wan;
		led-failsafe = &led_wan;
		led-upgrade = &led_wan;
	};

	chosen {
		// bootargs-append = " ubi.mtd=UBI_DEV";
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			gpios = <&tlmm 2 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		reset {
			label = "reset";
			gpios = <&tlmm 63 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_wan: wan {
			label = "ti04-708h:green:wan";
			gpios = <&tlmm 0 GPIO_ACTIVE_LOW>;
		};

		wlan {
			label = "ti04-708h:green:wlan";
			gpios = <&tlmm 58 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};
	};

	soc {
		rng@22000 {
			status = "okay";
		};

		ess-psgmii@98000 {
			status = "okay";
		};

		counter@4a1000 {
			compatible = "qcom,qca-gcnt";
			reg = <0x4a1000 0x4>;
		};

		tcsr@1949000 {
			compatible = "qcom,tcsr";
			reg = <0x1949000 0x100>;
			qcom,wifi_glb_cfg = <TCSR_WIFI_GLB_CFG>;
		};

		tcsr@194b000 {
			compatible = "qcom,tcsr";
			reg = <0x194b000 0x100>;
			qcom,usb-hsphy-mode-select = <TCSR_USB_HSPHY_HOST_MODE>;
		};

		ess_tcsr@1953000 {
			compatible = "qcom,tcsr";
			reg = <0x1953000 0x1000>;
			qcom,ess-interface-select = <TCSR_ESS_PSGMII>;
		};

		tcsr@1957000 {
			compatible = "qcom,tcsr";
			reg = <0x1957000 0x100>;
			qcom,wifi_noc_memtype_m0_m2 = <TCSR_WIFI_NOC_MEMTYPE_M0_M2>;
		};

		usb2@60f8800 {
			status = "okay";

			dwc3@6000000 {
				#address-cells = <1>;
				#size-cells = <0>;

				port@1 {
					reg = <1>;
					#trigger-source-cells = <0>;
				};
			};
		};

		crypto@8e3a000 {
			status = "okay";
		};

		watchdog@b017000 {
			status = "okay";
		};

		ess-switch@c000000 {
			status = "okay";

			// switch_lan_bmp = <0x3c>;
			// switch_wan_bmp = <0x02>;
		};

		edma@c080000 {
			status = "okay";
		};
	};
};

&blsp_dma {
	status = "okay";
};

&blsp1_spi1 { /* BLSP1 QUP1 */
	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	status = "okay";
	cs-gpios = <&tlmm 54 GPIO_ACTIVE_HIGH>,
		   <&tlmm 59 GPIO_ACTIVE_HIGH>;

	flash@0 {
		// compatible = "jedec,spi-nor", "n25q128a11";
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			nor-flash@0 {
				label = "nor";
				reg = <0x0 0x200000>;
				read-only;
			};

			partition@0 {
				label = "SBL1";
				reg = <0x0 0x40000>;
				read-only;
			};
			partition@40000 {
				label = "MIBIB";
				reg = <0x40000 0x20000>;
				read-only;
			};
			partition@60000 {
				label = "QSEE";
				reg = <0x60000 0x60000>;
				read-only;
			};
			partition@c0000 {
				label = "CDT";
				reg = <0xc0000 0x10000>;
				read-only;
			};
			partition@d0000 {
				label = "DDRPARAMS";
				reg = <0xd0000 0x10000>;
				read-only;
			};
			partition@e0000 {
				label = "APPSBLENV"; /* u-boot-env */
				reg = <0xe0000 0x10000>;
				read-only;
			};
			partition@f0000 {
				label = "APPSBL"; /* u-boot */
				reg = <0xf0000 0x80000>;
				read-only;
			};
			partition@170000 {
				label = "ART";
				reg = <0x170000 0x10000>;
				read-only;
			};
			/* 0x180000 - 0x200000 unused */
		};
	};

	spi-nand@1 {
		compatible = "spi-nand";
		reg = <1>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			nand-flash@0 {
				label = "ubi";
				reg = <0x0 0x8000000>;
				read-only;
			};

			partition@0 {
				label = "bank1";
				reg = <0x0 0x2000000>;
				read-only;
			};
			partition@3000000 {
				label = "bank2";
				reg = <0x3000000 0x2000000>;
				read-only;
			};
			partition@6300000 {
				label = "bank3";
				reg = <0x6300000 0x800000>;
				read-only;
			};
		};
	};
};

&blsp1_uart1 {
	pinctrl-0 = <&serial_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&cryptobam {
	status = "okay";
};

&gmac0 {
	// vlan_tag = <1 0x3c>;
};

&gmac1 {
	// vlan_tag = <2 0x02>;
};

&mdio {
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	status = "okay";
	// phy-reset-gpio = <&tlmm 62 GPIO_ACTIVE_LOW>;
	reset-gpios = <&tlmm 62 GPIO_ACTIVE_LOW>;
	reset-delay-us = <5000>;
};

&qpic_bam {
	status = "okay";
};

&tlmm {
	mdio_pins: mdio_pinmux {
		mux_mdio {
			pins = "gpio53";
			function = "mdio";
			bias-pull-up;
		};

		mux_mdc {
			pins = "gpio52";
			function = "mdc";
			bias-pull-up;
		};
	};

	serial_pins: serial_pinmux {
		mux {
			pins = "gpio60", "gpio61";
			function = "blsp_uart0";
			bias-disable;
		};
	};

	spi_0_pins: spi_0_pinmux {
		mux {
			pins = "gpio55", "gpio56", "gpio57";
			function = "blsp_spi0";
			drive-strength = <12>;
			bias-disable;
		};

		mux_cs {
			function = "gpio";
			pins = "gpio54", "gpio59";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};
};

&usb2_hs_phy {
	status = "okay";
};

&wifi0 {
	status = "okay";
	qcom,ath10k-calibration-variant = "TI04-708H";
};

&wifi1 {
	status = "okay";
	qcom,ath10k-calibration-variant = "TI04-708H";
};
