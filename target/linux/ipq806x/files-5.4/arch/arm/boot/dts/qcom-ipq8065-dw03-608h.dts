#include "qcom-ipq8065.dtsi"

#include <dt-bindings/input/input.h>

/ {
	model = "T&Ie DW03-608H";
	compatible = "tnie,dw03-608h", "qcom,ipq8065", "qcom,ipq8064";

	aliases {
		serial0 = &gsbi4_serial;
		led-boot = &led_wan_blue;
		led-failsafe = &led_wan_blue;
		led-upgrade = &led_wan_blue;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	keys {
		compatible = "gpio-keys";
		pinctrl-0 = <&button_pins>;
		pinctrl-names = "default";

		reset {
			label = "reset";
			gpios = <&qcom_pinmux 54 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&qcom_pinmux 65 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-0 = <&led_pins>;
		pinctrl-names = "default";

		led_wan_blue: wan-blue {
			label = "dw03-608h:blue:wan";
			gpios = <&qcom_pinmux 9 GPIO_ACTIVE_HIGH>;
		};

		wan-green {
			label = "dw03-608h:green:wan";
			gpios = <&qcom_pinmux 53 GPIO_ACTIVE_HIGH>;
		};

		wlan {
			label = "dw03-608h:green:wlan";
			gpios = <&qcom_pinmux 67 GPIO_ACTIVE_HIGH>;
		};
	};
};

&qcom_pinmux {
	led_pins: led_pins {
		mux {
			pins = "gpio9", "gpio53", "gpio67";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	button_pins: button_pins {
		mux {
			pins = "gpio54", "gpio65";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	uart_pins: uart_pins {
		mux {
			pins = "gpio10", "gpio11";
			function = "gsbi4";
			drive-strength = <12>;
			bias-disable;
		};
	};

	rpm_pins: rpm_pins {
		mux {
			pins = "gpio12", "gpio13";
			function = "gsbi4";
			drive-strength = <12>;
			bias-disable;
		};
	};

	nand_pins: nand_pins {
		disable {
			pins = "gpio34", "gpio35", "gpio36", "gpio37",
			       "gpio38";
			function = "nand";
			drive-strength = <10>;
			bias-disable;
		};

		pullups {
			pins = "gpio39";
			function = "nand";
			drive-strength = <10>;
			bias-pull-up;
		};

		hold {
			pins = "gpio40", "gpio41", "gpio42", "gpio43",
			       "gpio44", "gpio45", "gpio46", "gpio47";
			function = "nand";
			drive-strength = <10>;
			bias-bus-hold;
		};
	};

	mdio_pins: mdio_pins {
		mux {
			pins = "gpio0", "gpio1";
			function = "mdio";
			drive-strength = <8>;
			bias-disable;
		};

		clk {
			pins = "gpio1";
			input-disable;
		};

		reset {
			pins = "gpio63";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};
	};

	rgmii2_pins: rgmii2_pins {
		mux {
			pins = "gpio27", "gpio28", "gpio29", "gpio30",
			       "gpio31", "gpio32", "gpio51", "gpio52",
			       "gpio59", "gpio60", "gpio61", "gpio62";
			function = "rgmii2";
			drive-strength = <8>;
			bias-disable;
		};

		tx {
			pins = "gpio27", "gpio28", "gpio29", "gpio30",
			       "gpio31", "gpio32";
			input-disable;
		};
	};
};

&gsbi4 {
	qcom,mode = <GSBI_PROT_I2C_UART>;
	status = "okay";

	/*
	* The i2c device on gsbi4 should not be enabled.
	* On ipq806x designs gsbi4 i2c is meant for exclusive
	* RPM usage. Turning this on in kernel manifests as
	* i2c failure for the RPM.
	*/
};

&gsbi4_serial {
	status = "okay";

	pinctrl-0 = <&uart_pins>;
	pinctrl-names = "default";
};

&rpm {
	pinctrl-0 = <&rpm_pins>;
	pinctrl-names = "default";
};

&adm_dma {
	status = "okay";
};

&nand_controller {
	status = "okay";

	pinctrl-0 = <&nand_pins>;
	pinctrl-names = "default";

	nand@0 {
		reg = <0>;
		compatible = "qcom,nandcs";

		nand-ecc-strength = <4>;
		nand-bus-width = <8>;
		nand-ecc-step-size = <512>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "SBL1";
				reg = <0x0 0x40000>;
				read-only;
			};

			partition@40000 {
				label = "MIBIB";
				reg = <0x40000 0x140000>;
				read-only;
			};

			partition@180000 {
				label = "SBL2";
				reg = <0x180000 0x140000>;
				read-only;
			};

			partition@2c0000 {
				label = "SBL3";
				reg = <0x2c0000 0x280000>;
				read-only;
			};

			partition@540000 {
				label = "DDRCONFIG";
				reg = <0x540000 0x120000>;
				read-only;
			};

			partition@660000 {
				label = "SSD";
				reg = <0x660000 0x120000>;
				read-only;
			};

			partition@780000 {
				label = "TZ";
				reg = <0x780000 0x280000>;
				read-only;
			};

			partition@a00000 {
				label = "RPM";
				reg = <0xa00000 0x280000>;
				read-only;
			};

			partition@c80000 {
				label = "APPSBL";
				reg = <0xc80000 0x500000>;
				read-only;
			};

			partition@1180000 {
				label = "APPSBLENV";
				reg = <0x1180000 0x180000>;
			};

			partition@1300000 {
				label = "ART";
				reg = <0x1300000 0x140000>;
				read-only;
			};

			partition@1440000 {
				label = "ubi";
				reg = <0x1440000 0xebc0000>;
			};
		};
	};
};

&mdio0 {
	status = "okay";

	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	reset-gpios = <&qcom_pinmux 63 GPIO_ACTIVE_HIGH>;
	reset-delay-us = <2000>;

	switch@0 {
		reg = <0x0>;
		qca,ar8327-initvals = <
			0x0004 0x07600000  /* PAD0_MODE */
			0x0008 0x01000000  /* PAD5_MODE */
			0x000c 0x00000080  /* PAD6_MODE */
			0x0010 0x002613a0  /* POWER_ON_STRAP */
			0x007c 0x0000004e  /* PORT0_STATUS */
			0x0094 0x0000004e  /* PORT6_STATUS */
			0x00e0 0xc74164de  /* SGMII_CTRL */
			0x00e4 0x000aa545  /* MAC_POWER_SEL */
		>;
	};
};

&gmac1 {
	status = "okay";

	pinctrl-0 = <&rgmii2_pins>;
	pinctrl-names = "default";

	phy-mode = "rgmii";
	qcom,id = <1>;

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&gmac2 {
	status = "okay";

	phy-mode = "sgmii";
	qcom,id = <2>;

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&usb3_0 {
	status = "okay";
};

&dwc3_0 {
	#address-cells = <1>;
	#size-cells = <0>;

	port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb3_1 {
	status = "okay";
};

&dwc3_1 {
	#address-cells = <1>;
	#size-cells = <0>;

	port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&pcie0 {
	status = "okay";

	/delete-property/ perst-gpios;

	bridge@0,0 {
		reg = <0x00000000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;
		ranges;

		wifi@1,0 {
			compatible = "qcom,ath10k";
			reg = <0x00010000 0 0 0 0>;
		};
	};
};

&pcie1 {
	status = "okay";

	/delete-property/ perst-gpios;

	bridge@0,0 {
		reg = <0x00000000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;
		ranges;

		wifi@1,0 {
			compatible = "qcom,ath10k";
			reg = <0x00010000 0 0 0 0>;
		};
	};
};
