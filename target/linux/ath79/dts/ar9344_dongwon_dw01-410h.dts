#include "ar9344.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "dongwon,dw01-410h", "qca,ar9344";
	model = "Dongwon T&I DW01-410H";

	aliases {
		led-boot = &led_wan;
		led-failsafe = &led_wan;
		led-upgrade = &led_wan;
	};

	leds {
		compatible = "gpio-leds";

		wlan {
			label = "green:wlan";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		led_wan: wan {
			label = "green:wan";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		reset {
			label = "reset";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&ref {
	clock-frequency = <40000000>;
};

&spi {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x40000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x40000 0x10000>;
			};

			partition@50000 {
				label = "current";
				reg = <0x50000 0x7b0000>;
				read-only;
			};

			partition@800000 {
				compatible = "dongwon,uimage";
				label = "firmware";
				reg = <0x800000 0x7b0000>;
			};

			partition@fb0000 {
				label = "log";
				reg = <0xfb0000 0x10000>;
				read-only;
			};

			partition@fc0000 {
				label = "voc";
				reg = <0xfc0000 0x10000>;
				read-only;
			};

			partition@fd0000 {
				label = "nvbackup";
				reg = <0xfd0000 0x10000>;
				read-only;
			};

			partition@fe0000 {
				label = "nvram";
				reg = <0xfe0000 0x10000>;
				read-only;
			};

			art: partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x10000>;
				read-only;
			};
		};
	};
};

&eth0 {
	status = "okay";

	/* default for ar934x, except for 1000M */
	pll-data = <0x06000000 0x00000101 0x00001616>;

	mtd-mac-address = <&art 0x6>;

	phy-mode = "rgmii";

	fixed-link {
		speed = <1000>;
		full-duplex;
		pause;
	};
};

&mdio0 {
	status = "okay";

	switch@0 {
		reg = <0x0>;
		qca,ar8327-initvals = <
			0x0004 0x07600000  /* PORT0_PAD_CTRL */
			0x0050 0xcf37cf37  /* LED_CTRL0 */
			0x0054 0x00000000  /* LED_CTRL1 */
			0x0058 0x00000000  /* LED_CTRL2 */
			0x005c 0x0030c300  /* LED_CTRL3 */
			0x007c 0x0000007e  /* PORT0_STATUS */
		>;
	};
};

&pcie {
	status = "okay";

	wifi@0,0 {
		compatible = "pci168c,0033";
		reg = <0x0000 0 0 0 0>;
		qca,no-eeprom;
		mtd-mac-address = <&art 0x0>;
		mtd-mac-address-increment = <4>;
	};
};

&usb {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;

	port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x1000>;
	mtd-mac-address = <&art 0x0>;
	mtd-mac-address-increment = <3>;
};
